#include <stdio.h>

void  disp_2digit_odd_sum_tens7();

int main() 
{
     disp_2digit_odd_sum_tens7();
    return 0;
}
void  disp_2digit_odd_sum_tens7() 
{
    int i, sum = 0;
    for(i = 70; i <= 79; i++) 
    {
        if(i % 2 == 1) 
        {
            printf("%d\n",i);
            sum += i;
        }
    }
    printf("\n%d", sum);
}
